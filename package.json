    "@angular/router": "20.3.7",
    "@google/genai": "latest",
    "rxjs": "7.8.1",
    "tailwindcss": "latest",
    "three": "0.164.1"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "gh-pages": "^6.3.0",
